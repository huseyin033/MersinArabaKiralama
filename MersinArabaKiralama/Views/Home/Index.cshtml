  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="loginRequiredModalLabel">Giriş Yapmanız Gerekiyor</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Kapat"></button>
      </div>
      <div class="modal-body text-center">
        Tüm araçları görebilmek için lütfen giriş yapınız.
      </div>
      <div class="modal-footer justify-content-center">
        <a href="/Identity/Account/Login" class="btn btn-primary">Giriş Yap</a>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Kapat</button>
      </div>
    </div>
  </div>
</div>

<div class="container my-4 text-center">
    
</div>

<script>
    var isAuthenticated = @(SignInManager.IsSignedIn(User).ToString().ToLower());
    document.getElementById('seeAllCarsBtn').addEventListener('click', function () {
        if (!isAuthenticated) {
            var modal = new bootstrap.Modal(document.getElementById('loginRequiredModal'));
            modal.show();
        } else {
            window.location.href = '/Car/Index';
        }
    });
</script>

