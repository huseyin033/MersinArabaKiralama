@using Microsoft.AspNetCore.Identity
@inject SignInManager<MersinArabaKiralama.Models.ApplicationUser> SignInManager
<div class="container mb-5">
    <h2 class="text-center mb-4" style="font-family: 'Montserrat', sans-serif; font-weight: 700; color: #2c3e50;">Hayallerinizdeki Araca Bir Adım Uzakta!</h2>
</div>

@{ 
    ViewData["Title"] = "Mersin Araç Kiralama";
    var cars = ViewBag.Cars as List<MersinArabaKiralama.Models.Car>;
    var campaigns = ViewBag.Campaigns as List<string>;
}



@if (SignInManager.IsSignedIn(User))
{
    <!-- Hızlı Arama -->
    <div class="container mb-5">
        <form class="row g-3 justify-content-center" method="get" action="/Car">
            <div class="col-md-3">
                <input type="text" name="brand" class="form-control" placeholder="Marka ile Ara (örn: Renault)">
            </div>
            <div class="col-md-3">
                <select name="isAvailable" class="form-select">
                    <option value="">Tüm Durumlar</option>
                    <option value="true">Müsait</option>
                    <option value="false">Dolu</option>
                </select>
            </div>
            <div class="col-md-2">
                <button type="submit" class="btn btn-primary w-100">Ara</button>
            </div>
        </form>
    </div>

    <!-- Kampanya/Duyurular -->
    @if (campaigns != null && campaigns.Count > 0)
    {
        <div class="container mb-4">
            <div class="alert alert-info text-center">
                <strong>Kampanyalar:</strong>
                <ul class="mb-0">
                    @foreach (var c in campaigns)
                    {
                        <li>@c</li>
                    }
                </ul>
            </div>
        </div>
    }

    <!-- Araç Listesi -->
    <div class="container mb-5">
        <h3 class="mb-4 text-center">Öne Çıkan Araçlar</h3>
        <div class="row row-cols-1 row-cols-md-4 g-2">
            @if (cars != null && cars.Count > 0)
            {
                foreach (var car in cars)
                {
                    <div class="col">
                        <div class="card h-100 shadow-sm">

                            <div class="card-body">
                                <h5 class="card-title">@car.Brand @car.Model</h5>
                                <p class="card-text">Yıl: @car.Year<br />Günlük: <strong>@car.DailyPrice ₺</strong></p>
                                <span class="badge bg-@(car.IsAvailable ? "success" : "danger")">@(car.IsAvailable ? "Müsait" : "Dolu")</span>
                            </div>
                            <div class="card-footer bg-white border-0 text-center">
                                <a href="/Car/Details/@car.Id" class="btn btn-outline-primary btn-sm">Detaylar</a>
                            </div>
                        </div>
                    </div>
                }
            }
            else
            {
                <div class="col-12 text-center">
                    <div class="alert alert-warning">Şu anda öne çıkan araç bulunmamaktadır.</div>
                </div>
            }
        </div>
        <div class="row mt-4">
            <div class="col text-center">
                <button id="seeAllCarsBtn" class="btn btn-lg btn-primary px-5">Tüm Arabaları Gör</button>
            </div>
        </div>
    </div>
}

<!-- Yaptığımız İşler -->
<div class="container mb-5">
    <h3 class="mb-4 text-center">Yaptığımız İşler</h3>
    <div class="row text-center g-2">
        <div class="col-md-4 mb-3">
            <i class="bi bi-car-front display-4 text-info"></i>
            <h5>Kurumsal Araç Kiralama</h5>
            <p>Şirketlere özel filo çözümleri ve uzun dönem kiralama hizmetleri.</p>
        </div>
        <div class="col-md-4 mb-3">
            <i class="bi bi-people display-4 text-success"></i>
            <h5>Bireysel Araç Kiralama</h5>
            <p>Kısa ve uzun süreli bireysel araç kiralama seçenekleri.</p>
        </div>
        <div class="col-md-4 mb-3">
            <i class="bi bi-geo-alt display-4 text-warning"></i>
            <h5>Havalimanı Transfer</h5>
            <p>Mersin ve çevresinde havalimanı transfer hizmetleri.</p>
        </div>
    </div>
</div>

<!-- Neden Bizi Seçmelisiniz? -->
<div class="container mb-5">
    <h3 class="mb-4 text-center">Neden Bizi Seçmelisiniz?</h3>
    <div class="row text-center g-2">
        <div class="col-md-4 mb-3">
            <i class="bi bi-shield-check display-4 text-primary"></i>
            <h5>Güvenilir Hizmet</h5>
            <p>Yıllardır Mersin'de güvenle araç kiralama hizmeti sunuyoruz.</p>
        </div>
        <div class="col-md-4 mb-3">
            <i class="bi bi-cash-coin display-4 text-success"></i>
            <h5>Uygun Fiyatlar</h5>
            <p>Her bütçeye uygun, şeffaf ve rekabetçi fiyatlar.</p>
        </div>
        <div class="col-md-4 mb-3">
            <i class="bi bi-clock-history display-4 text-warning"></i>
            <h5>7/24 Destek</h5>
            <p>Her zaman ulaşabileceğiniz müşteri hizmetleri.</p>
        </div>
    </div>
</div>





<!-- Müşteri Yorumları -->
<div class="container mb-5">
    <h3 class="mb-4 text-center">Müşteri Yorumları</h3>
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Araçlar çok temiz ve bakımlıydı. Hizmetten çok memnun kaldım!"</p>
                        <footer class="blockquote-footer">Ayşe K.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Fiyatlar çok uygun ve çalışanlar çok ilgiliydi. Herkese tavsiye ederim."</p>
                        <footer class="blockquote-footer">Mehmet D.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Rezervasyon işlemi çok hızlıydı, arabam zamanında teslim edildi."</p>
                        <footer class="blockquote-footer">Elif S.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Müşteri hizmetleri çok yardımcı oldu, tekrar tercih edeceğim."</p>
                        <footer class="blockquote-footer">Burak T.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Araçlar yeni ve konforluydu, ailemle çok rahat ettik."</p>
                        <footer class="blockquote-footer">Zeynep Y.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Fiyat/performans açısından bölgedeki en iyi firma diyebilirim."</p>
                        <footer class="blockquote-footer">Ali E.</footer>
                    </blockquote>
                </div>
            </div>
            <div class="card shadow-sm mb-3">
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>"Online rezervasyon sistemi çok pratik, tavsiye ederim."</p>
                        <footer class="blockquote-footer">Gülşah M.</footer>
                    </blockquote>
                </div>
            </div>
        </div>
    </div>
</div>

@if (!SignInManager.IsSignedIn(User))
{
    <div class="container text-center mt-5 mb-5">
        <p class="lead">
            Araba kiralamak ve arabalarımızla ilgili detaylı bilgilere erişmek için lütfen <a href="/Identity/Account/Register">giriş yapınız</a>.
        </p>
    </div>
}

